{
    "name": "crims/portal-api",
    "description": "Centralized authentication and middleware library for CRIMS",
    "version": "1.0.0",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Portal\\": "app/",
            "Common\\": "app/"
        }
    },
    "require-dev": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Portal\\Providers\\AuthServiceProvider",
                "Portal\\Providers\\MiddlewareServiceProvider"
            ]
        }
    }
}